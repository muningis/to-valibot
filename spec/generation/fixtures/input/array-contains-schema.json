{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ArrayContains",
  "type": "object",
  "properties": {
    "tags": {
      "type": "array",
      "items": { "type": "string" },
      "contains": { "type": "string", "pattern": "^important" }
    },
    "numbers": {
      "type": "array",
      "items": { "type": "number" },
      "contains": { "type": "number", "minimum": 10 }
    }
  },
  "required": ["tags"]
}
