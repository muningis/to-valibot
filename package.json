{
  "name": "to-valibot",
  "version": "0.0.7",
  "license": "MIT",
  "author": "Rokas Muningis",
  "repository": {
    "type": "git",
    "url": "https://github.com/muningis/to-valibot"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE.md",
    "README.md",
    "client.js",
    "client.cjs",
    "client.d.ts"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./client": {
      "import": {
        "types": "./dist/client/index.d.ts",
        "default": "./dist/client/index.js"
      },
      "require": {
        "types": "./dist/client/index.d.cts",
        "default": "./dist/client/index.cjs"
      }
    }
  },
  "scripts": {
    "test": "vitest --typecheck",
    "coverage": "vitest run --coverage --isolate",
    "lint": "eslint \"lib/**/*.ts*\" && tsc --noEmit",
    "lint.fix": "eslint \"lib/**/*.ts*\" --fix",
    "format": "prettier --write ./lib",
    "format.check": "prettier --check ./lib",
    "build": "tsup"
  },
  "peerDependencies": {
    "typescript": "5.8.2",
    "valibot": "1.2.0",
    "yaml": "2.7.0"
  },
  "devDependencies": {
    "@eslint/js": "9.21.0",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/node": "22.13.5",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsdoc": "50.6.3",
    "eslint-plugin-security": "3.0.1",
    "eslint": "9.21.0",
    "globals": "16.0.0",
    "prettier": "3.5.3",
    "tsup": "8.4.0",
    "typescript-eslint": "8.25.0",
    "vitest": "3.0.9",
    "yaml": "2.7.0"
  },
  "packageManager": "pnpm@9.7.1+sha512.faf344af2d6ca65c4c5c8c2224ea77a81a5e8859cbc4e06b1511ddce2f0151512431dd19e6aff31f2c6a8f5f2aced9bd2273e1fed7dd4de1868984059d2c4247"
}
